        Kimi no Suizō wo Tabetai
      ]
      ar_001: null
      hu_HU: null
      he_IL: null
      id_ID: null
      en_US: [
        Let Me Eat Your Pancreas
      ]
      ja_JP: null
      de_DE: null
      ko_KR: null
      fr_FR: null
      pt_PT: null
      ru_RU: [
        Хочу съесть твою поджелудочную железу
      ]
