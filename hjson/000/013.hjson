    //  REGEX: ^[\w\s\SЀ-ӿ]+$
    ru_RU: Сомали и лесной дух

    # es_ES [str] <nab>
    # = Serial title in Spanish (or other accent).
    //  REGEX: ^[\w\s\SÀ-ÿ]+$
    es_ES: Somali to Mori no Kamisama

